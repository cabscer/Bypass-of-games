        <script>
          checkdeledaosetting()
        function query(param) {
            const urlParams = new URLSearchParams(window.location.search);
            return urlParams.get(param);
    }   
    if (query("url") === null){
            document.getElementById("frameframeframe").src = "/embed.html#" + sessionStorage.getItem("proxyurl")
    }
        else{
            sessionStorage.setItem("proxyurl", query("url"))
            document.getElementById("frameframeframe").src = "/embed.html#" + sessionStorage.getItem("proxyurl")
            const url = new URL(window.location);
            url.searchParams.delete('url');
            window.history.replaceState({}, '', url);
    }

        document.getElementById('reload').addEventListener('click', function() {
            var iframe = document.getElementById('frameframeframe');
            iframe.src = iframe.src;
        });

        function goFullscreen(id) {
        var element = document.getElementById(id);
        if (element.mozRequestFullScreen) {
          element.mozRequestFullScreen();
        } else if (element.webkitRequestFullScreen) {
          element.webkitRequestFullScreen();
        }
    }
            function toggleScript() {
                const script = document.getElementById("discord");
                if (script) {
                    script.remove();
                    document.getElementsByTagName("widgetbot-crate")[0].remove();
                    document.getElementById("chat").src = "/stuff/icons/comment-solid.svg";
                } else {
                    var create = document.createElement("script");
                    create.id = "discord";
                    create.src = "https://cdn.jsdelivr.net/npm/@widgetbot/crate@3";
                    create.innerHTML = "new Crate({ server: '975549006841270312', channel: '1042980843704025158' });";
                    document.body.appendChild(create);
                    document.getElementById("chat").src = "/stuff/icons/commentblocked-solid.svg";
                }
            }
        </script>
